*{
	margin: 0px;
	padding: 0px;
}
input[type="button"], input[type="submit"], input[type="reset"] {

-webkit-appearance: none;

}

textarea {  -webkit-appearance: none;}
/* 底框样式  */
#addemployee{
	width:100%;
	height:50px;
	bottom:0px;
	position: fixed;
	display:-webkit-flex;
	background:linear-gradient(to bottom, #ededed 5%, #f5f5f5 100%);
}
#inputID{
	margin-left:10px;
	margin-right:10px;
 	align-self:center;
	border-radius:6px;
	flex:4;
	height:30px;
}
/* 加号样式 */
.className{
	margin-right:10px;
	align-self:center;
	line-height:40px;
	height:20px;
	width:20px;
	color:#777777;
	background-color:#ededed;
	font-size:40px;
	border-radius:10px;
	font-weight:normal;
	font-family:Arial;
	background:linear-gradient(to bottom, #ededed 5%, #f5f5f5 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ededed', endColorstr='#f5f5f5',GradientType=0);
	 /*  border:1px solid #dcdcdc; */
	box-shadow:-webkit-inset 0px 0px 0px 0px #ffffff;
	text-align:center;
	display:inline-block;
	text-decoration:none;
	-moz-transform:rotate(-90deg);
	-webkit-transform:rotate(-90deg);
	transform:rotate(-90deg);
}