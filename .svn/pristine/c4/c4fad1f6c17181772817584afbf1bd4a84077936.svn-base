@CHARSET "UTF-8";

* {
	margin: 0px;
	padding: 0px;
	font-family: "microsoft yahei";
	font-size: 15px;
}

#background {
	width: 100%;
	background-color: rgb(255, 255, 255);
}

#title {
	position:fixed;
	top:0;
	left:0;
	height: 35px;
	width: 100%;
	line-height: 35px;
}

.choose {
	text-align: center;
	border-bottom: 2px solid black;
}

#template {
	float: left;
	width: 50%;
	color: rgb(0, 204, 0);
	border-color: rgb(0, 204, 0);
}

#detailed {
	width: 50%;
	float: left;
}

.content {
	margin-top: 40px;
	margin-left: 2px;
	margin-right: 2px;
	background-color: rgb(236, 236, 236);
}

.OperatingMessage {
	height: 35px;
	width: 100%;
	margin-top: 10px;
	text-align: center;
	line-height: 35px;
}

.message {
	margin-left: 2px;
	margin-right: 2px;
	background-color: rgb(255, 255, 255);
	width: 98%;
}

#chooseTemplate {
	margin-top: 40px;
}

.specific {
	text-align: right;
	line-height: 40px;
	width: 60%;
}

.caption {
	height: 40px;
	line-height: 40px;
	text-align: center;
	width: 30%;
}

.more {
	width: 10%;
	margin-top: 9px;
	text-align: center;
}

.anniu {
	height: 40px;
	width: 96%;
	display: block;
	color: rgb(255, 255, 255);
	background-color: rgb(0, 236, 0);
	text-align: center;
	border-radius: 8px;
	margin: 30px auto;
}

#fillIn {
	margin: 15px auto;
}

.detial {
	height: 40px;
	text-align: right;
	line-height: 30px;
	width: 64%;
	display: inline;
}
#Receiver-detial{
	width: 20%;
	float:right;
}
#hint{
	height: 40px;
	text-align: left;
	line-height: 40px;
	width: 44%;
	display: inline;
	color:rgb(220,220,220);
	font-size:13px;
	float:left;
}
.row{
	height: 40px;
	width: 100%;
	border-bottom: 1px solid rgb(192, 192, 192);
	border-left: 1px solid rgb(192, 192, 192);
	border-right: 1px solid rgb(192, 192, 192);
}

.name-list{
	display:none;
	width:100%;
	margin:0;
	padding:0;
	line-height: 40px;
	border-bottom: 1px solid rgb(192, 192, 192);
	border-left: 1px solid rgb(192, 192, 192);
	border-right: 1px solid rgb(192, 192, 192);
}
li{
	padding-left:15px;
	height:40px;
	line-height: 40px;
	display:inline-block;
}
#second {
	border-top: 1px solid rgb(192, 192, 192);
}

#first {
	border-top: 1px solid rgb(192, 192, 192);
}

#chooseProject {
	width: 80%;
	border: 0px;
	margin-left: 10%;
	z-index: 99;
	position: fixed;
	text-align: center;
	background: rgb(255, 255, 255);
	top: 81px;
	border-radius: 5px;
	height: auto;
}
#dianjixianshi{
	display:none;
}
.title-name,.templateName,.section,.division,.name,.depename {
	height: 30px;
	line-height: 30px;
}

#nameTitle {
	background: rgb(0, 204, 0);
	width: 80%;
	border-radius: 5px;
	position: fixed;
	top: 81px;
}

#type,#type1,#type2,#type3,#type4,#type5 {
	background: rgb(0, 204, 0);
	border-radius: 5px;
	position: fixed;
	width: 80%;
	top: 81px;
}

.allRow {
	margin-top: 30px;
}

option {
	text-align: right;
}

input {
	height: 38px;
	width: 100%;
	text-align: right;
	line-height: 38px;
	border: 0px;
	outline: none;
}

.xinxi {
	height: 40px;
	line-height: 40px;
}

#bigBackground {
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	background-color: rgba(0, 0, 0, 0.2);
	z-index: 99;
}

#photos {
	height: 90px;
	width: 100%;
}

.photo {
	height: 70px;
	width: 20%;
	margin-top: 5px;
	margin-left: 4px;
}

#imghead {
	height: 70px;
	width: 23%;
	margin-top: 5px;
	margin-left: 4px;
}

#imgUpload {
	height: 90px;
	width: 25%;
	border-radius: 40px;
	float: left;
	margin-top: 5px;
	background-color: #000;
}

#hah { 
	display: none;
	top: 0;
	left: 0;
	z-index: 999;
	justify-content: center;
	align-items: center;
	overflow: auto;
}

#operate {
	height: 35px;
	width: 100%;
	position: fixed;
	top: 0px;
	display: inline-block;
	background-color: rgb(0, 0, 0);
}

.delete {
	height: 35px;
	width: 30px;
	float: right;
	margin-right: 5px;
}

.return {
	float: left;
	margin-left: 5px;
	height: 35px;
	width: 30px;
}

* 去苹果按钮样式 * /
input[type="button"],input[type="submit"],input[type="reset"] {
	-webkit-appearance: none;
}

textarea {
	-webkit-appearance: none;
}

/* *{margin: 0;padding: 0;} */
li {
	list-style-type: none;
}

a,input {
	outline: none;
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

#choose {
	display: none;
}

canvas {
	width: 100%;
	border: 1px solid #000000;
}

#upload {
	display: block;
	height: 40px;
	line-height: 40px;
	cursor: pointer;
}

.touch {
	background-color: #ddd;
}

