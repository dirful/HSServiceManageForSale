      		trade = #{entity.trade},
      		progress = #{entity.progress},
      		remarks = #{entity.remarks},
      		typeID = #{entity.typeID},
      		picID = #{entity.picID},
      		offerID= #{entity.offerID},
      		ReceiverID= #{entity.ReceiverID}
      	]]>
      	<where>
