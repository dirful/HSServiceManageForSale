package com.cqut.entity.ServiceMessageType;

import java.util.Date;
import com.cqut.service.tableCreator.ID;

public class ServiceMessageType{
	
	@ID
	private String  ID;
	private String ServiceMessageCode;
	private String ServiceMessageName;
	private String description;
	private int level;
	private String parentID;
	private int state;
	
	
	public String getID() {
		return ID;
	}	
	
	public void setID(String ID) {
		this.ID = ID;
	}
	public String getServiceMessageCode() {
		return ServiceMessageCode;
	}	
	
	public void setServiceMessageCode(String ServiceMessageCode) {
		this.ServiceMessageCode = ServiceMessageCode;
	}
	public String getServiceMessageName() {
		return ServiceMessageName;
	}	
	
	public void setServiceMessageName(String ServiceMessageName) {
		this.ServiceMessageName = ServiceMessageName;
	}
	public String getDescription() {
		return description;
	}	
	
	public void setDescription(String description) {
		this.description = description;
	}
	public int getLevel() {
		return level;
	}	
	
	public void setLevel(int level) {
		this.level = level;
	}
	public String getParentID() {
		return parentID;
	}	
	
	public void setParentID(String parentID) {
		this.parentID = parentID;
	}
	public int getState() {
		return state;
	}	
	
	public void setState(int state) {
		this.state = state;
	}
	
	@Override
	public String toString() {
		return   ID  + "\t" +  ServiceMessageCode  + "\t" +  ServiceMessageName  + "\t" +  description  + "\t" +  level  + "\t" +  parentID  + "\t" +  state  + "\t"  ;
	}
}
