* {
	padding: 0px;
	margin: 0px;
	font-size: 16px;
}
input[type="button"], input[type="submit"], input[type="reset"] {
	-webkit-appearance: none;
}
body{
	position: relative;
	top: 35px;
	background-color: #EBEBEB;
}
#head {
	width: 100%;
	border-bottom: 1px solid #797979;
	height: 35px;
	background-color: white;
	position: fixed;
	top: 0;
	box-shadow: 0px 1px #C0C0C0;
}

.head_part {
	float: left;
	border-right: 1px solid #797979;
	width: 33%;
	height: 35px;
	line-height: 35px;
	text-align: center;
}

.head_activity {
	box-shadow: 0px 2px 0px rgb(0, 204, 0);
	color: rgb(0, 204, 0);
}

#center {
	width: 100%;
	background-color: white;
}

#center_part {
	width: 100%;
	margin:0 auto;
}

#auditlist {
	padding-top: 15px;
	width: 100%;
	list-style: none;
	background-color: #EBEBEB;
}

#auditlist li {
	padding: 15px 3% 10px 3%;
	margin-bottom: 15px;
	background-color: white;
	box-shadow: 0px 0px 3px rgb(192, 192, 192);
}

.li_of_1 {
	display: inline-block;
	width: 74%;
}

.li_of_2 {
	display: inline-block;
	color: #333;
	vertical-align: bottom;
}

.li_of_2 div {
	height: 30px;
	width: 60px;
	font-size: 13px;
	border: 1px solid #797979;
	margin-bottom: 10px;
	text-align: center;
	line-height: 30px;
}

.li_of_2 div a {
	font-size: 13px;
}

.through {
	background-color: #009900;
}

.withdraw {
	background-color: #009900;
}

.projecname {
	margin-right: 5px;
	color: #333333;
	display: inline-block;
    height: 18px;
    width: 58%;
    white-space:nowrap; 
    overflow:hidden; 
    text-overflow:ellipsis;
}

.auditinfo {
	margin: 5px 0px 0px 5px;
	width: 88%;
}

.content {
	font-size: 13px;
	color: #999999;
}

.auditinfo a {
	display: block;
}
#backgound_div{
	position: fixed;
	top: 0px;
	width: 100%;
	height: 100%;
	background-color: black;
	opacity: 0.2;	
	filter: alpha(opacity=20);
	-moz-opacity: 0.2;
	-khtml-opacity: 0.2;
	display: none;
}
#approvaldetails{
	position: fixed;
	top: 0px;
	width: 100%;
	height: 100%;
	display: none;
}
#ad_inside{
	height: 60%;
	width: 75%;
	margin: 36% auto;
	background-color: #FFF;
	border-radius: 10px;
	-moz-border-radius: 10px;
	-khtml-border-radius: 10px;
	box-shadow: 0px 0px 2px #797979;
	-moz-box-shadow: 0px 0px 2px #797979;
	-khtml-box-shadow: 0px 0px 2px #797979;
	padding: 5px 15px 0px;
}
#adi{
	height: 100%;
}
#ad_of_1{
	width: 100%;
	height: 75%;
	border: 1px solid #c0c0c0;
	border-radius: 5px;
	-moz-border-radius: 5px;
	-khtml-border-radius: 5px;
	margin: 5% 0px;
	box-shadow: inset 0px 0px 1px #797979;
	-moz-box-shadow: inset 0px 0px 1px #797979;
	-khtml-box-shadow: inset 0px 0px 1px #797979;
	overflow: scroll;
}
#ad_of_2{
	width: 100%;
}
.button{
	background-color: rgb(0, 204, 0);
	color: white;
	display: inline-block;
	width: 46%;
	border: 1px solid rgb(0, 204, 0);
	border-radius: 5px;
	-moz-border-radius: 5px;
	-khtml-border-radius: 5px;
	line-height: 35px;
    text-align: center;
    font-size: 18px;
}
#ad_return{
	float: right;
	width: 100%;
}
#ad_content{
	margin: 10px;
}
#ad_content p{
	font-size: 13px;
}
#resultdetails{
	position: fixed;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
	display: none;
}
#rd_inside{
	height: 70%;
	width: 75%;
	margin: 21% auto;
	background-color: #FFF;
	border-radius: 10px;
	-moz-border-radius: 10px;
	-khtml-border-radius: 10px;
	box-shadow: 0px 0px 2px #797979;
	-moz-box-shadow: 0px 0px 2px #797979;
	-khtml-box-shadow: 0px 0px 2px #797979;
	padding: 5px 15px 0px;
}
#rdi{
	height: 100%;
}
#rd_of_1{
	width: 100%;
	height: 75%;
	border: 1px solid #c0c0c0;
	border-radius: 5px;
	-moz-border-radius: 5px;
	-khtml-border-radius: 5px;
	margin: 5% 0px;
	box-shadow: inset 0px 0px 1px #797979;
	-moz-box-shadow: inset 0px 0px 1px #797979;
	-khtml-box-shadow: inset 0px 0px 1px #797979;
	overflow: scroll;
}
#rd_of_2{
	width: 100%;
	height: 15%;
}
#rd_determine{
	float: left;
}
#rd_content{
	margin: 10px;
	height: 90%;
}
#rd_cancel{
	float: right;
}
#rd_content_textarea{
	font-size: 13px;
	width: 100%;
	height: 100%;
	display: inline-block;
	border: none;
	resize:none;
}
#auditprompt{
	position: fixed;
	bottom: 0px;
	/* margin: 127% 0px auto; */
	width: 100%;
	text-align: center;
	z-index: 3;
	display: none;
	margin-bottom: 20px;
}
.ap_inside{
	margin: 0px auto;
	text-align: center;
	max-width: 73%;
	border-radius: 12px;
	background-color: #000;
	color: #FFF;
	font-size: 16px;
	padding: 2px 18px;
	display: inline-block;
	/* filter:alpha(opacity=0); 
	-moz-opacity: 0; 
	opacity: 0; */
}
.bagintransition{
	transition: opacity 0.5s;
	-moz-transition: opacity 0.5s;
	-webkit-transition: opacity 0.5s;
	-o-transition: opacity 0.5s;
}
.finaltransition{
	transition: opacity 1s;
	-moz-transition: opacity 1s;
	-webkit-transition: opacity 1s;
	-o-transition: opacity 1s;
}
.show{
	filter:alpha(opacity=100); 
	-moz-opacity: 1; 
	opacity: 1;
}
.hide{
	filter:alpha(opacity=0); 
	-moz-opacity: 0; 
	opacity: 0;
}
.sto{
	background-image: url("../image/null.png");
	background-size: 137px 200px;
	background-repeat: no-repeat;
}
#loading{
	position: fixed;
	top:0px;
	margin: 0 auto;
	width: 100%;
	display: none;
}
#loading_side{
	text-align: center;
	width: 50%;
	margin: 0 auto;
	height: 40px;
	background-color: black;
	color: white;
	line-height: 40px;
	opacity: 0.9;
	border-radius: 13px;
}
.notouch{
	overflow: hidden;
}
.touch{
	overflow: auto;
}