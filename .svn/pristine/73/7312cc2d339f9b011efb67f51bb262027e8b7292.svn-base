    <meta name="keywords" content="keyword1,keyword2,keyword3">
    <meta name="description" content="this is my page">
    <meta name="content-type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width = device-width ,initial-scale = 1.0 , minimum-scale = 1.0 , user-scalable =  no ">
	<meta name="format-detection" content="telephone=no">
	
	<link rel="stylesheet" type="text/css" href="../css/ProjectInformationCollectionAndManagement.css">
	
	<script type="text/javascript" src="../../../frame/jquery/jquery-2.1.0.js"></script>
	<script type="text/javascript" src="../js/ProjectInformationCollectionAndManagement.js"></script>
