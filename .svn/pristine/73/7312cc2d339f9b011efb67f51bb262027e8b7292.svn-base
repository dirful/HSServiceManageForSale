<body>
	<input type="button" id="add" value="新建项目" style="display:none"/>
	<div id="projectBox"></div>
	
	<div class="detailBox" id="detailBox" >
		<div class="detailData_box">
			<div class="data_row2" id="projectCode2">项目编码<div class="data2"></div></div>
			<div class="data_row2" id="projectName2">项目名称<div class="data2"></div></div>
			<div class="data_row2" id="bigClass2">所属大类<div class="data2"></div></div>
