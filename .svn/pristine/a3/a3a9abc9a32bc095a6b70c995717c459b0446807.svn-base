*{
    margin:0px;
    padding:0px;
    font-size: 15px;
}
input[type="button"], input[type="submit"], input[type="reset"] {

-webkit-appearance: none;

}

textarea {  -webkit-appearance: none;}
body{
    margin:0px 8px;
}
body>div:nth-of-type(1){
    display:-webkit-flex;
    display: -moz-box;  /* Firefox */ 
    display: -ms-flexbox;    /* IE10 */ 
    display: -webkit-box;    /* Safari */  
    display: -webkit-flex;    /* Chrome, WebKit */ 
    display: box;  
    display: flexbox;  
    display: flex;
    width:100%;
}
body>div:nth-of-type(1)>div{
    height:40px;
    -webkit-flex:1;
    flex:1;
    moz-box-flex:1.0; /* Firefox */
    -webkit-box-flex:1.0; /* Safari 和 Chrome */
    box-flex:1.0;
    margin:0px 4px;
    border-bottom:solid rgba(153,153,153,1) 2px;
    display: inline-block;
    display: -webkit-box;    /* Safari */  
    display: -webkit-flex;    /* Chrome, WebKit */ 
    display: box;  
    display: flexbox;  
    display: flex;
    display: -ms-flexbox;    /* IE10 */ 
    display: -moz-box;  /* Firefox */ 
    -webkit-box-pack: center;
    -webkit-box-align: center;

}
body>div:nth-of-type(1)>div:first-child{
    color:rgba(0,204,0,1);
    border-bottom:2px solid rgba(0,204,0,1);
}
body>div:nth-of-type(1)>div>span{
    width:100%;
    -moz-box-flex:1.0; /* Firefox */
    -webkit-box-flex:1.0; /* Safari 和 Chrome */
    box-flex:1.0;
    -webkit-flex:1;
    flex:1;
    text-align:center; 
    -webkit-align-self:center;
    -webkit-box-pack:center;
    -webkit-box-align:center;
    box-pack:center;
    box-align:center;
    -moz-box-pack:center;
    -moz-box-align:center;
}
body>div:nth-of-type(2)>div:nth-of-type(1){
    display:-webkit-flex;
    display: -moz-box;  /* Firefox */ 
    display: -ms-flexbox;    /* IE10 */ 
    display: -webkit-box;    /* Safari */  
    display: -webkit-flex;    /* Chrome, WebKit */ 
    display: box;  
    display: flexbox;  
    display: flex;
    height:380px;
    -webkit-flex-direction:column;
    flex-direction:column;
    -moz-box-orient:vertical;
    -webkit-box-orient:vertical;
    box-orient:vertical;
    margin:10px 4px;
}
body>div:nth-of-type(2)>div:nth-of-type(1)>div{
    width:100%;
    -webkit-flex:1;
    flex:1;
    -moz-box-flex:1.0; /* Firefox */
    -webkit-box-flex:1.0; /* Safari 和 Chrome */
    box-flex:1.0;
    display: -moz-box;  /* Firefox */ 
    display: -ms-flexbox;    /* IE10 */ 
    display: -webkit-box;    /* Safari */  
    display: -webkit-flex;    /* Chrome, WebKit */ 
    display: box;  
    display: flexbox;  
    display: flex;
    border-bottom:1px solid rgba(153,153,153,1);
    justify-content:space-between;
    -webkit-justify-content:space-between;
    -webkit-box-align:center;
    -webkit-box-pack:justify;
}
body>div:nth-of-type(2)>div:nth-of-type(1)>div>*:last-child{
    color:rgba(153,153,153,1);
}
body>div:nth-of-type(2)>div:nth-of-type(1)>div:last-child{
    margin-bottom:150px;
}
body>div:nth-of-type(2)>div:nth-of-type(1)>div>*{
    align-self:center;
    -webkit-align-self:center;
    -webkit-box-pack:center;
    -webkit-box-align:center;
    box-pack:center;
    box-align:center;
    -moz-box-pack:center;
    -moz-box-align:center;
    text-align:center; 
}
/*第二页的*/
body>div:nth-of-type(2)>div:nth-of-type(2){
    display: -moz-box;  /* Firefox */ 
    display: -ms-flexbox;    /* IE10 */ 
    display: -webkit-box;    /* Safari */  
    display: -webkit-flex;    /* Chrome, WebKit */ 
    display: box;  
    display: flexbox;  
    display: flex;
    height:380px;
    -webkit-flex-direction:column;
    flex-direction:column;
    -moz-box-orient:vertical;
    -webkit-box-orient:vertical;
    box-orient:vertical;
    margin:10px 4px;
}
body>div:nth-of-type(2)>div:nth-of-type(2)>div{
    width:100%;
    flex:1;
    -webkit-flex:1;
    -moz-box-flex:1.0; /* Firefox */
    -webkit-box-flex:1.0; /* Safari 和 Chrome */
    box-flex:1.0;
    display: -moz-box;  /* Firefox */ 
    display: -ms-flexbox;    /* IE10 */ 
    display: -webkit-box;    /* Safari */  
    display: -webkit-flex;    /* Chrome, WebKit */ 
    display: box;  
    display: flexbox;  
    display: flex;
    border-bottom:1px solid rgba(153,153,153,1);
    justify-content:space-between;
    -webkit-justify-content:space-between;
    -webkit-box-align:center;
    -webkit-box-pack:justify;
}
body>div:nth-of-type(2)>div:nth-of-type(2)>div:nth-last-of-type(3){
    border-bottom:0px solid rgba(153,153,153,1);
}
body>div:nth-of-type(2)>div:nth-of-type(2)>div:nth-last-of-type(1){
    border-bottom:0px solid rgba(153,153,153,1);
}
body>div:nth-of-type(2)>div:nth-of-type(2)>div>*:not(div):last-child {
    color:rgba(153,153,153,1);
}
body>div:nth-of-type(2)>div:nth-of-type(2)>div:nth-last-of-type(1){
    margin-bottom:50px;
}
body>div:nth-of-type(2)>div:nth-of-type(2)>div>*{
    -webkit-align-self:center;
    text-align:center; 
/*  -webkit-box-pack:center; */
    -webkit-box-align:center;
/*  box-pack:center; */
    box-align:center;
/*  -moz-box-pack:center; */
    -moz-box-align:center;
}

/*第二页s的表框*/
body>div:nth-of-type(2)>div:nth-of-type(2)>div:nth-last-of-type(3){
    flex:initial;
    -webkit-flex:initial;
    -moz-box-flex:initial; /* Firefox */
    -webkit-box-flex:initial; /* Safari 和 Chrome */
    box-flex:initial;
    height:90px;
    overflow:auto;
    border:1px solid rgba(0,0,0,1);
    -webkit-flex-direction:column;
    flex-direction:column;
    -moz-box-orient:vertical;
    -webkit-box-orient:vertical;
    box-orient:vertical;
    -webkit-justify-content: initial;
    justify-content: initial;
    margin-top:10px;
}
body>div:nth-of-type(2)>div:nth-of-type(2)>div:nth-last-of-type(3)>div{
    width:100%;
    line-height:30px;
    display: -moz-box;  /* Firefox */ 
    display: -ms-flexbox;    /* IE10 */ 
    display: -webkit-box;    /* Safari */  
    display: -webkit-flex;    /* Chrome, WebKit */ 
    display: box;  
    display: flexbox;  
    display: flex;
    border-bottom:1px solid rgba(153,153,153,1);
    justify-content:space-between;
    -webkit-justify-content:space-between;
    -webkit-box-pack:justify;
}
body>div:nth-of-type(2)>div:nth-of-type(2)>div:nth-last-of-type(3)>div>div{
/*  display:inline-block; */ 
    margin:0px 10px 0px 10px;
    -webkit-align-self:center;
    align-self:center;
    -webkit-box-pack:center;
    -webkit-box-align:center;
    box-pack:center;
    box-align:center;
    -moz-box-pack:center;
    -moz-box-align:center;
    text-align:center; 
    font-size:1em;
}
body>div:nth-of-type(2)>div:nth-of-type(2)>div:nth-last-of-type(3)>div>div:nth-of-type(1) {
    margin-right: 140px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 100px;
}
body>div:nth-of-type(2)>div:nth-of-type(2)>div:nth-last-of-type(3)>div>*:nth-last-of-type(1){
    color:rgba(153,153,153,1);
}
/*提交按键 */
#submit{
    padding:13px 0px;
    height:10px;
    line-height:10px;
    text-align:center;
    color:rgba(255,255,255,1);
    width:100%;
    border:rgba(121,121,121,1) 1px solid;
    background:rgba(0,204,0,1);
}
/*报价单名称*/
@media screen and (min-width:330px) and (max-width:630px){
    #project{
            height:15px;
            display:inline-block;
            width:190px;
            color:rgba(153,153,153,1);
            background:#fff;
            text-align:right;
    }
}

@media screen and (max-width:330px){ /*大于等于1440*/
    /*这里是特定的样式表*/
    #project{
            height:15px;
            display:inline-block;
            background:#fff;
            width:170px;
            color:rgba(153,153,153,1);
            text-align:right;
    }
}
/*星号*/
#start{
    color:red;
}
/*是否选择自定义审核人的勾*/
#employeeOpen{
	display:inline-block;
	color:#fff;
	position: relative;
    bottom: 6px;
    right: 3px;
    font-family: "Microsoft YaHei";
    transform: rotate(225deg) skew(0deg,180deg) rotateX(180deg);
    -ms-transform: rotate(225deg) skew(0deg,180deg) rotateX(180deg);      /* IE 9 */
    -webkit-transform: rotate(225deg) skew(0deg,180deg) rotateX(180deg);   /* Safari and Chrome */
    -o-transform: rotate(225deg) skew(0deg,180deg) rotateX(180deg);        /* Opera */
    -moz-transform: rotate(225deg) skew(0deg,180deg) rotateX(180deg);
}
/*勾的外框*/
#employeeOpenFather{
    padding-left: 5px;
    height: 10px;
    width: 5px;
    border: 1px solid #000;
}
/*加入新的报价详情*/
.addInfo{
    display: -moz-box;  /* Firefox */ 
    display: -ms-flexbox;    /* IE10 */ 
    display: -webkit-box;    /* Safari */  
    display: -webkit-flex;    /* Chrome, WebKit */ 
    display: box;  
    display: flexbox;  
    display: flex;
    border:0px #000 solid;
    width:100%;
    margin-top:10px;
    height: 30px !important;
    -webkit-flex:initial;
    flex:initial;
    -moz-box-flex:initial; /* Firefox */
    -webkit-box-flex:initial; /* Safari 和 Chrome */
    box-flex:initial;
    background:#ededed;
    -webkit-box-orient:horizontal;
    /*  -webkit-box-pack:center; */
    -webkit-box-align:center;
    /*  box-pack:center; */
    box-align:center;
    /*  -moz-box-pack:center; */
    -moz-box-align:center;
    border-bottom:none;
}
.addInfo>input{
    height:20px;
    -webkit-flex:1;
    flex:1;
    -moz-box-flex:1; /* Firefox */
    -webkit-box-flex:1; /* Safari 和 Chrome */
    box-flex:1;
    margin-bottom:5px;
}
#addInfoName{
    border-bottom:none;
}
#addInfoPrice{
    margin-top:-10px;
}
/* 加号样式 */
.className{
    -webkit-align-self:center;
    align-self:center;
    -webkit-box-pack:center;
    -webkit-box-align:center;
    box-pack:center;
    box-align:center;
    -moz-box-pack:center;
    -moz-box-align:center;
    line-height:32px;
    height:30px;
    width:30px;
    color:#777777;
    background-color:#ededed;
    font-size:32px;
    border-radius:0px;
    font-weight:normal;
    font-family:Arial;
    background:linear-gradient(to bottom, #ededed 5%, #f5f5f5 100%);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ededed', endColorstr='#f5f5f5',GradientType=0);
     /*  border:1px solid #dcdcdc; */
    box-shadow:-webkit-inset 0px 0px 0px 0px #ffffff;
    text-align:center;
    display:inline-block;
    text-decoration:none;
}
.delete{
    -webkit-align-self:center;
    -webkit-box-pack:center;
    -webkit-box-align:center;
    box-pack:center;
    box-align:center;
    -moz-box-pack:center;
    -moz-box-align:center;
    line-height:32px;
    height:30px;
    width:30px;
    color:#777777;
    font-size:32px;
    border-radius:0px;
    font-weight:normal;
    font-family:Arial;
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ededed', endColorstr='#f5f5f5',GradientType=0);
     /*  border:1px solid #dcdcdc; */
    box-shadow:-webkit-inset 0px 0px 0px 0px #ffffff;
    text-align:center;
    display:inline-block;
    text-decoration:none;
    -webkit-transform:rotate(-45deg);
    transform:rotate(-45deg);
}
/*选择框的特效*/
#templetList,#typeBList,#typeLList,#projectList,#employeeList{
    z-index:9999;
    position:absolute;
    display:none;
    overflow:auto;
    min-height:15px; 
    max-height:100px;
    width:100px;
    background:#ececec;
}
#typeBFather,#typeLFather,#templetFather{
    z-index:0;
}
#typeB,#typeL,#templet{
    height:15px;
    width:100px;
}
.chooseThis[rpg="typeL"],.chooseThis[rpg="templet"]{
    display:none;
}
