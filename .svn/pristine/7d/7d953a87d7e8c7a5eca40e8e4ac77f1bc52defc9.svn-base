@CHARSET "UTF-8";
* {
	margin: 0px;
	padding: 0px;
	font-family: "microsoft yahei";
	font-size: 15px;
}
body{
	background-color: rgb(236, 236, 236); 
	height: 100%;
}
.inner{
	background-color: rgb(236, 236, 236); 
}
#background {
	width: 100%;
	background-color: rgb(255, 255, 255);
}
#bkgd1{
	position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: rgba(0,0,0,0.2);
    z-index: 99;
}
#title {
	height: 35px;
	width: 100%;
	position:fixed;
	top:0px;
	background-color: white;
}
.choose {
	height: 35px;
	line-height: 35px;
	text-align: center;
	border-bottom: 2px solid black;
	float:left;
}

#send {
	width: 42%;
	color:rgb(0,204,0);
	border-color: rgb(0,204,0);
}

#receive {
	width: 42%;
}
#new{
	width:16%;
}
#newAdd{
	height:20px;
	width:20px;
	margin-top: 6px;
}
.content {
	padding-top: 2px;
	margin-top: 35px;
	margin-left: 2px;
	margin-right: 2px;
	background-color: rgb(236, 236, 236); 
}
.Operating{
	height:35px;
	width:100%;
	margin-top: 10px;
	text-align: right;
	position:fixed;
	top:0px;
}
.message {
	margin-left: 2px;
    margin-right: 2px;
    background-color: rgb(255, 255, 255);
    margin-top: 20px;
    
}
.row{
	height:35px;
	width:100%;
}
.caption{
	height:30px;
	width:25%;
	line-height:30px;
	diaplay:inline;
	text-align: left;
	padding-left:5%;
}
.detial{
	height:30px;
	text-align: right;
	line-height:30px;
	width:65%;
	display:inline;
}
.xinxi{
	text-align: right;
	padding-right:5%;
}
.name{
	height: 40px;
	line-height:40px;
	width: 100%;
	border-bottom: 1px solid rgb(192, 192, 192);
	border-left: 1px solid rgb(192, 192, 192);
	border-right: 1px solid rgb(192, 192, 192);
}
#nameTitle{
	background: rgb(0,204,0);
	text-align: center;
}
#dianjixianshi{
	position:fixed;
	top:60px;
	display:none;
}
#chooseProject{
	width:98%;
	height:250px;
	border:0px;
	margin-left:1%;
	z-index: 99;
	position: fixed;
	top:80px;
	/* text-align:center; */
	background: rgb(255,255,255);
  /*   margin-top: 81px; */
}
.pictureRow{
	height:70px;
	width:100%;
}
.photo{
	height:100%;
	width:25%;
	float: left;
}
.addPicture{
	float:left;
}
.addRow{
	background: rgb(236, 236, 236);
	top:35px;
	position:fixed;
	height:30px;
}
#hah {
	display: none;
	top: 0;
	left:0;
	z-index:999;
	justify-content: center;
	align-items: center;
	overflow:auto;
}
.pull{
	height:30px;
	line-height:30px;
	width:120px;
	border-radius:5px;
	text-align: center;
	background-color:rgb(0,204,0);
	color:rgb(255,255,255);
	margin-left: auto;
	margin-right: auto;
}
* 去苹果按钮样式 */
input[type="button"] {
	-webkit-appearance: none;
}

textarea {
	-webkit-appearance: none;
}